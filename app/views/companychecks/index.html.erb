<p id="notice"><%= notice %></p>

<h1>Companychecks</h1>

<table>
  <thead>
    <tr>
      <th>Iscompany</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companychecks.each do |companycheck| %>
      <tr>
        <td><%= companycheck.iscompany %></td>
        <td><%= companycheck.user %></td>
        <td><%= link_to 'Show', companycheck %></td>
        <td><%= link_to 'Edit', edit_companycheck_path(companycheck) %></td>
        <td><%= link_to 'Destroy', companycheck, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Companycheck', new_companycheck_path %>
